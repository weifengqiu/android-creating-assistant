public class WeatherToday {
/**
 * {"weaid":"165",
 * "days":"2017-03-27",
 * "week":"星期一",
 * "cityno":"guangzhou",
 * "citynm":"广州",
 * "cityid":"101280101",
 * "temperature":"23℃/14℃",
 * "temperature_curr":"22℃",
 * "humidity":"40%",
 * "weather":"多云",
 * "weather_curr":"晴",
 *"weather_icon":"http://api.k780.com:88/upload/weather/d/0.gif",
 * "weather_icon1":"",
 * "wind":"南风",
 * "winp":"2级",
 * "temp_high":"23",
 * "temp_low":"14",
 * "temp_curr":"22",
 * "humi_high":"0",
 * "humi_low":"0",
 * "weatid":"1",
 * "weatid1":"",
 * "windid":"5",
 * "winpid":"203"}
 */
 
 //注意这里成员变量的命名要与json数据中的一致
    private String days;
    private String temperature_curr;
    private String temperature;
    private String weather;
    private String wind;
    private String winp;
    private String weather_icon;
    private String weather_icon1;

    public String getDays() {
        return days;
    }

    public void setDays(String days) {
        this.days = days;
    }

    public String getTemperature_curr() {
        return temperature_curr;
    }

    public void setTemperature_curr(String temperature_curr) {
        this.temperature_curr = temperature_curr;
    }

    public String getTemperature() {
        return temperature;
    }

    public void setTemperature(String temperature) {
        this.temperature = temperature;
    }

    public String getWeather() {
        return weather;
    }

    public void setWeather(String weather) {
        this.weather = weather;
    }

    public String getWind() {
        return wind;
    }

    public void setWind(String wind) {
        this.wind = wind;
    }

    public String getWinp() {
        return winp;
    }

    public void setWinp(String winp) {
        this.winp = winp;
    }

    public String getWeather_icon() {
        return weather_icon;
    }

    public void setWeather_icon(String weather_icon) {
        this.weather_icon = weather_icon;
    }

    public String getWeather_icon1() {
        return weather_icon1;
    }

    public void setWeather_icon1(String weather_icon1) {
        this.weather_icon1 = weather_icon1;
    }



}
